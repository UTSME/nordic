{
    "version": 2,
    "cmakeMinimumRequired": {
        "major": 3,
        "minor": 20
    },
    "configurePresets": [
        {
            "name": "build",
            "displayName": "Build for nRF9160 DK NRF52840",
            "generator": "Ninja",
            "binaryDir": "${sourceDir}/build",
            "cacheVariables": {
                "NCS_TOOLCHAIN_VERSION": "NONE",
                "BOARD": "nrf9160dk_nrf52840@1.1.3",
                "CACHED_CONF_FILE": "${sourceDir}/prj.conf"
            }
        }
    ]
}